  <DataSets>
    <DataSet Name="DS_AlbumSales">
      <Query>
        <DataSourceName>DSMusic</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Album">
            <Value>=Parameters!Album.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!StartDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!EndDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <CommandText>SELECT
    al.pkAlbum,
    al.Album,
    ar.Artist,
    tl.Title,
    als.Salesdate,
    als.SalesAmt
FROM dbo.Album al
    LEFT JOIN dbo.TrackList  tl ON tl.fkAlbum       = al.pkAlbum
    LEFT JOIN dbo.AlbumSales als ON als.fkAlbum     = al.pkAlbum
    LEFT JOIN dbo.Artist     ar  ON ar.pkArtist     = tl.fkArtist   
WHERE
    al.pkAlbum IN (@Album)
    AND als.Salesdate Between  @StartDate and @EndDate
ORDER by al.Album ASC</CommandText>
      </Query>
      <Fields>
        <Field Name="pkAlbum">
          <rd:TypeName>System.Int32</rd:TypeName>
          <DataField>pkAlbum</DataField>
        </Field>
        <Field Name="Artist">
          <rd:TypeName>System.String</rd:TypeName>
          <DataField>Artist</DataField>
        </Field>
        <Field Name="Album">
          <rd:TypeName>System.String</rd:TypeName>
          <DataField>Album</DataField>
        </Field>
        <Field Name="Title">
          <rd:TypeName>System.String</rd:TypeName>
          <DataField>Title</DataField>
        </Field>
        <Field Name="Salesdate">
          <rd:TypeName>System.DateTime</rd:TypeName>
          <DataField>Salesdate</DataField>
        </Field>
        <Field Name="SalesAmt">
          <rd:TypeName>System.Decimal</rd:TypeName>
          <DataField>SalesAmt</DataField>
        </Field>
      </Fields>
    </DataSet>
